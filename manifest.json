{
    "background": {
        "scripts": [
            "js/jquery-2.0.0.min.js",
            "js/jquery.cookie.js",
            "js/background.js",
            "js/util.js",
            "js/comm.js"
        ]
    },
    "description": "找到你想要的种子",
    "icons": {
        "16": "icons/av48.png",
        "48": "icons/av48.png",
        "128": "icons/av96.png"
    },
    "manifest_version": 2,
    "name": "找种子",
    "options_page": "options/option.html",
    "browser_action": {
        "default_icon": {
            "16": "icons/av48.png",
            "24": "icons/av48.png",
            "32": "icons/av48.png"
        },
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "alarms",
        "tabs",
        "cookies",
        "notifications",
        "http://thzbt.co/*",
        "http://thzu.cc/*",
        "http://thzthz.cc/*",
        "http://www.dmm.co.jp/*",
        "http://pics.dmm.co.jp/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "version": "0.0.0.1"
}
